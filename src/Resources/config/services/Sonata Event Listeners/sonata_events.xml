<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service class="Sylius\Bundle\UiBundle\Block\BlockEventListener"
                 id="skyboundtech_wholesale_ruleset.block_event_listener.admin.layout.javascripts"
        >
            <tag name="kernel.event_listener" event="sonata.block.event.sylius.admin.layout.javascripts"
                 method="onBlockEvent"/>
            <argument>@SkyBoundTechSyliusWholesaleSuitePlugin/Admin/_javascripts.html.twig</argument>
        </service>
        <service class="Sylius\Bundle\UiBundle\Block\BlockEventListener"
                 id="skyboundtech_wholesale_ruleset.block_event_listener.admin.layout.stylesheets"
        >
            <tag name="kernel.event_listener" event="sonata.block.event.sylius.admin.layout.stylesheets"
                 method="onBlockEvent"/>
            <argument>@SkyBoundTechSyliusWholesaleSuitePlugin/Admin/_stylesheets.html.twig</argument>
        </service>
    </services>
</container>
