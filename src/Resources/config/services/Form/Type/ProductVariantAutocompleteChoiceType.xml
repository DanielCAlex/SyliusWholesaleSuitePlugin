<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="sky_bound_tech.sylius_wholesale_suite_plugin.form.type.product_variant_autocomplete_choice_type"
                 class="SkyBoundTech\SyliusWholesaleSuitePlugin\Form\Type\ProductVariantAutocompleteChoiceType"
                 autowire="false">
            <tag name="form.type"/>
            <argument type="service" id="Symfony\Component\Routing\Generator\UrlGeneratorInterface"/>
            <argument type="string" id="Sylius\Component\Core\Model\ProductVariant::class"/>
        </service>
    </services>
</container>
